{
  "name": "Streamlit Dark",
  "dataColors": [
    "#204020", "#3BA372", "#68C588", "#95D5AA",
    "#A3F7B5", "#5DADE2", "#2980B9", "#AF7AC5",
    "#E59866", "#E74C3C"
  ],
  "background": "#0b1220",
  "foreground": "#FFFFFF",
  "tableAccent": "#204020",

  "visualStyles": {
    "*": {
      "*": {
        "title": [ { "color": { "solid": { "color": "#E6E6E6" } }, "fontSize": 13 } ],
        "labels": [ { "color": { "solid": { "color": "#CFCFCF" } }, "fontSize": 11 } ],
        "categoryLabels": [ { "color": { "solid": { "color": "#CFCFCF" } }, "fontSize": 11 } ],
        "background": [ { "color": { "solid": { "color": "#0f1720" } }, "transparency": 0 } ]
      }
    },

    "page": {
      "*": {
        "background": [ { "color": { "solid": { "color": "#0b1220" } }, "transparency": 0 } ]
      }
    },

    "card": {
      "*": {
        "calloutValue": [ { "color": { "solid": { "color": "#E6E6E6" } }, "fontSize": 24 } ],
        "categoryLabels": [ { "color": { "solid": { "color": "#CFCFCF" } }, "fontSize": 12 } ]
      }
    },

    "barChart": {
      "*": {
        "dataPoint": [ { "defaultColor": { "solid": { "color": "#3BA372" } } } ],
        "categoryAxis": [ { "labelColor": { "solid": { "color": "#CFCFCF" } } } ],
        "valueAxis": [ { "labelColor": { "solid": { "color": "#CFCFCF" } } } ]
      }
    },

    "lineChart": {
      "*": {
        "categoryAxis": [ { "labelColor": { "solid": { "color": "#CFCFCF" } } } ],
        "valueAxis": [ { "labelColor": { "solid": { "color": "#CFCFCF" } } } ],
        "dataPoint": [ { "lineColor": { "solid": { "color": "#68C588" } } } ]
      }
    },

    "filledMap": {
      "*": {
        "categoryLabels": [ { "color": { "solid": { "color": "#CFCFCF" } } } ]
      }
    }
  }
}
